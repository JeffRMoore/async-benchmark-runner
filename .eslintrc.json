{
    "env": {
      "node": true,
      "es6" : true,
      "jest" : true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
    "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
      "comma-dangle": "off",
      "no-plusplus": "off",
      "no-use-before-define": "off",
      "no-continue": "off",
      "import/prefer-default-export": "off"
    }
}